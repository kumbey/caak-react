{
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "caakmnlayerAPI",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true,
      "env": "prod"
    }
  ],
  "permissions": {
    "api": {
      "caakmn": [
        "Query",
        "Mutation",
        "Subscription"
      ]
    },
    "storage": {
      "UserTotal:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "GroupTotal:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "Notification:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  }
}